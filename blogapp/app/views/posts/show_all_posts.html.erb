<h1><%= @user.username %></h1>

<% @posts.each do |post| %>
	<h3><%= post.title %></h3><br>
	<h2><%= post.content %></h3><br>
	<% if post.is_a? UserPicturePost %>
	<img src="<%= post.image_url %>"></img>
	<% end %>
	<% if post.is_a? UserVideoPost %>
		<div class="flex-video">
        	<iframe width="420" height="315" src="//www.youtube.com/embed/fdB74OeA2CA" frameborder="0" allowfullscreen></iframe>
		</div>
	<% end %>
<% end %>